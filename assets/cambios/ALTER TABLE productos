ALTER TABLE productos
ADD modelo VARCHAR(50),
ADD anio INT(4),
ADD version VARCHAR(50),
ADD tipo VARCHAR(30),
ADD color VARCHAR(30),
ADD puertas INT(1),
ADD combustible VARCHAR(20),
ADD transmision VARCHAR(20),
ADD traccion VARCHAR(10),
ADD placa VARCHAR(20),
ADD tipo_vehiculo VARCHAR(30),
ADD vin VARCHAR(50),
ADD motor VARCHAR(50),
ADD kilometraje INT,
ADD pais_origen VARCHAR(50),
ADD fecha_importacion DATE,
ADD usado VARCHAR(5),
ADD dueno_anterior VARCHAR(100),
ADD cedula_rif VARCHAR(20),
ADD titulo_propiedad VARCHAR(255),
ADD factura_original VARCHAR(255),
ADD revision_tecnica VARCHAR(255),
ADD permiso_circulacion VARCHAR(255);
